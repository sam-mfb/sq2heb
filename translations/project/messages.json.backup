    },
    {
      "logicFile": "72.agilogic",
      "messageNumber": 15,
      "original": "It doesn't look like much from here.",
      "translation": "זה לא נראה הרבה מכאן.",
      "notes": "",
      "placeholders": []
    },
    {
      "logicFile": "72.agilogic",
      "messageNumber": 16,
      "original": "It's not dark enough here to be effective.",
      "translation": "זה לא מספיק חשוך כאן כדי להיות יעיל.",
      "notes": "",
      "placeholders": []
    },
    {
      "logicFile": "73.agilogic",
      "messageNumber": 1,
      "original": "You are not near enough to one.",
      "translation": "אתה לא קרוב מספיק לאחד.",
